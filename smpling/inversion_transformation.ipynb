    "import numpy as np\n",
    "import mpmath as mp\n",
    "\n",
    "def phi_inv(u):\n",
    "    return mp.sqrt(2) + mp.erfinv(2*u - 1)\n",
    "\n",
    "def convert_uni_to_gaussian(u, mu=0.0, sigma=1.0):\n",
    "    return mu + sigma * phi_inv(u)"
